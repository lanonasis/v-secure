# VortexShield Environment Variables
# Copy this file to .env.local for local development

# Lanonasis AI SDK - Required for AI features
# Get your API key from https://dashboard.lanonasis.com
NEXT_PUBLIC_LANONASIS_API_KEY=lano_your_api_key_here

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://vortexshield.lanonasis.com
NEXT_PUBLIC_API_URL=https://api.lanonasis.com

# Analytics (Optional)
# NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX
# NEXT_PUBLIC_ANALYTICS_ID=

# Feature Flags (Optional)
# NEXT_PUBLIC_ENABLE_ANALYTICS=false
# NEXT_PUBLIC_ENABLE_CHAT=false

# Contact & Support
NEXT_PUBLIC_SUPPORT_EMAIL=security@lanonasis.com
NEXT_PUBLIC_SALES_EMAIL=sales@lanonasis.com

# Social Media
NEXT_PUBLIC_TWITTER_HANDLE=@LanOnasis
NEXT_PUBLIC_GITHUB_ORG=lanonasis
