{
  "name": "@vortex-secure/mcp-sdk",
  "version": "0.2.0",
  "description": "MCP SDK for secure secret access and API routing with Vortex Secure. Supports Web, CLI, and Node.js environments.",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/types/client/index.d.ts",
        "default": "./dist/esm/client/index.js"
      },
      "require": {
        "types": "./dist/types/client/index.d.ts",
        "default": "./dist/cjs/client/index.js"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/types/index.d.ts",
        "default": "./dist/esm/types/index.js"
      },
      "require": {
        "types": "./dist/types/types/index.d.ts",
        "default": "./dist/cjs/types/index.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/types/utils/index.d.ts",
        "default": "./dist/esm/utils/index.js"
      },
      "require": {
        "types": "./dist/types/utils/index.d.ts",
        "default": "./dist/cjs/utils/index.js"
      }
    }
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/fix-cjs.js",
    "build:types": "tsc -p tsconfig.types.json",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "files": [
    "dist/",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "keywords": [
    "mcp",
    "secrets",
    "security",
    "ai-agents",
    "vortex-secure",
    "model-context-protocol",
    "api-router",
    "credential-management",
    "zero-trust"
  ],
  "author": "The Fixer Initiative",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fixer-initiative/vortex-secure"
  },
  "bugs": {
    "url": "https://github.com/fixer-initiative/vortex-secure/issues"
  },
  "homepage": "https://github.com/fixer-initiative/vortex-secure#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
