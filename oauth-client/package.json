{
  "name": "@lanonasis/oauth-client",
  "version": "2.0.2",
  "type": "module",
  "description": "OAuth and API Key authentication client for Lan Onasis MCP integration",
  "license": "MIT",
  "author": "Lan Onasis",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser.mjs",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanonasis/v-secure.git",
    "directory": "oauth-client"
  },
  "bugs": {
    "url": "https://github.com/lanonasis/v-secure/issues"
  },
  "homepage": "https://github.com/lanonasis/v-secure/tree/main/oauth-client",
  "exports": {
    ".": {
      "browser": "./dist/browser.mjs",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.mjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.cjs"
    },
    "./cookies": {
      "types": "./dist/cookies/constants.d.ts",
      "import": "./dist/cookies/constants.mjs",
      "require": "./dist/cookies/constants.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/browser.d.ts"
      ],
      "react": [
        "./dist/react/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "cookies": [
        "./dist/cookies/constants.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --watch --config tsup.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src"
  },
  "keywords": [
    "oauth",
    "mcp",
    "lanonasis",
    "supabase",
    "api-key",
    "cli",
    "desktop"
  ],
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "eventsource": "^2.0.2",
    "keytar": "^7.9.0",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.17.0",
    "globals": "^16.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.1",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
