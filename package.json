{
  "name": "@lanonasis/security-service",
  "version": "1.0.0",
  "description": "Enterprise-grade security service for secrets, API keys, and access control",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "npm run test:packages",
    "test:packages": "npm ci --ignore-scripts --prefix oauth-client && npm ci --ignore-scripts --prefix security-sdk && npm ci --ignore-scripts --prefix web && npm ci --ignore-scripts --prefix vortex-secure/packages/vortex-mcp-sdk && npm run test --prefix oauth-client && npm run test --prefix security-sdk && npm run test --prefix web && npm run test --prefix vortex-secure/packages/vortex-mcp-sdk",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "./scripts/migrate.sh",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "security",
    "secrets",
    "api-keys",
    "encryption",
    "access-control",
    "compliance",
    "audit",
    "mcp",
    "enterprise"
  ],
  "author": "Seye Derick",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.6",
    "@eslint/js": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript-eslint": "^8.52.0",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
