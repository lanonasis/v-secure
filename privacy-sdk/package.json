{
  "name": "@lanonasis/privacy-sdk",
  "version": "1.0.0",
  "description": "Enterprise-grade privacy utilities: PII detection, data masking, tokenization, and GDPR compliance. Part of the LanOnasis Security Suite.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "privacy",
    "pii",
    "data-masking",
    "anonymization",
    "gdpr",
    "ccpa",
    "compliance",
    "security",
    "tokenization",
    "lanonasis",
    "vortexshield"
  ],
  "author": "LanOnasis <privacy@lanonasis.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lanonasis/v-secure.git",
    "directory": "privacy-sdk"
  },
  "homepage": "https://vortexshield.lanonasis.com",
  "bugs": {
    "url": "https://github.com/lanonasis/v-secure/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.40.0",
    "tsup": "^8.5.1",
    "typescript": "^5.4.0",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "related": {
    "@lanonasis/security-sdk": "Encryption, key management, API key generation",
    "@lanonasis/security-shield": "Edge-layer bot protection and WAF",
    "@lanonasis/mem-intel-sdk": "Memory intelligence and context management"
  }
}
